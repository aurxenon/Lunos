{
    "configurations": [
        {
            "name": "Kernel",
            "includePath": [
                "${workspaceFolder}/Kernel/Arch/x86/Include/**",
                "${workspaceFolder}/Kernel/Drivers/**",
                "${workspaceFolder}/Kernel/Filesystems/**",
                "${workspaceFolder}/Kernel/Include/**",
                "${workspaceFolder}/Kernel/**",
                "${workspaceFolder}/Kernel/System/**",
                "${workspaceFolder}/Libs/**"
